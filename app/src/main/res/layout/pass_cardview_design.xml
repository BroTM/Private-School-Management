<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">
    <android.support.v7.widget.CardView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="8dp"
        android:elevation="5dp">
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/developer_layout">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/app_title"
                android:layout_marginTop="16dp"
                android:textSize="32dp"
                android:text="ကိုယ်ပိုင်ကျောင်း"
                android:elevation="5dp"
                android:textStyle="bold"
                android:textColor="@color/colorPrimaryDark"
                android:layout_centerHorizontal="true" />
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="2016-2017"
                android:id="@+id/acdamic_year"
                android:layout_marginLeft="90dp"
                android:layout_below="@+id/app_title"
                android:layout_marginTop="8dp"
                android:textSize="16dp"/>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/acdamic"
                android:layout_toRightOf="@+id/acdamic_year"
                android:layout_below="@+id/app_title"
                android:layout_marginTop="8dp"
                android:layout_marginLeft="10dp"
                />
            <RelativeLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/manyD"
                android:layout_below="@id/acdamic_year"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="0dp"
                >
                <RelativeLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"

                    android:layout_marginLeft="0dp"
                    android:id="@+id/d6_layout"
                    android:layout_marginTop="32dp"

                    >
                    <RelativeLayout
                        android:layout_width="60dp"
                        android:layout_height="60dp"
                        android:id="@+id/d6"
                        android:layout_marginLeft="0dp"
                        android:background="@drawable/develop_pass_image"
                        >
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:elevation="6dp"
                            android:textStyle="bold"
                            android:textSize="32dp"
                            android:text="6D"
                            android:textColor="@color/white"
                            android:layout_centerInParent="true"/>
                    </RelativeLayout>
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/d6"
                        android:layout_marginTop="4dp"
                        android:id="@+id/student_6d"
                        android:layout_marginLeft="8dp"
                        android:text="6ယောက်"/>
                </RelativeLayout>

                <RelativeLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"

                    android:layout_marginLeft="16dp"
                    android:layout_marginTop="32dp"
                    android:layout_toRightOf="@+id/d6_layout"
                    android:id="@+id/d5_layout"
                    >
                    <RelativeLayout
                        android:layout_width="60dp"
                        android:layout_height="60dp"
                        android:id="@+id/d5"
                        android:layout_marginLeft="8dp"

                        android:background="@drawable/develop_pass_image"
                        >
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:elevation="6dp"
                            android:textStyle="bold"
                            android:textSize="32dp"
                            android:text="5D"
                            android:textColor="@color/white"
                            android:layout_centerInParent="true"/>
                    </RelativeLayout>
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/d5"
                        android:id="@+id/student_5d"
                        android:layout_marginTop="4dp"
                        android:layout_marginLeft="12dp"
                        android:text="6ယောက်"/>
                </RelativeLayout>



                <RelativeLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"

                    android:layout_marginLeft="16dp"
                    android:layout_toRightOf="@+id/d5_layout"
                    android:layout_marginTop="32dp"
                    android:id="@+id/d4_layout"

                    >
                    <RelativeLayout
                        android:layout_width="60dp"
                        android:layout_height="60dp"
                        android:id="@+id/d4"

                        android:layout_marginLeft="8dp"
                        android:background="@drawable/develop_pass_image"
                        >
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:elevation="6dp"
                            android:textStyle="bold"
                            android:textSize="32dp"
                            android:text="4D"
                            android:textColor="@color/white"
                            android:layout_centerInParent="true"/>
                    </RelativeLayout>
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/d4"
                        android:layout_marginTop="4dp"
                        android:id="@+id/student_4d"
                        android:layout_marginLeft="12dp"
                        android:text="6ယောက်"/>
                </RelativeLayout>
            </RelativeLayout>
            <RelativeLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@id/manyD"
                android:layout_centerHorizontal="true"
                android:id="@+id/lowD">
                <RelativeLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"

                    android:id="@+id/d3_layout"
                    android:layout_marginTop="16dp"


                    >
                    <RelativeLayout
                        android:layout_width="60dp"
                        android:layout_height="60dp"
                        android:id="@+id/d3"
                        android:layout_marginLeft="8dp"
                        android:background="@drawable/develop_pass_image"
                        >
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:elevation="6dp"
                            android:textStyle="bold"
                            android:textSize="32dp"
                            android:text="3D"

                            android:textColor="@color/white"
                            android:layout_centerInParent="true"/>
                    </RelativeLayout>
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/d3"
                        android:layout_marginTop="4dp"
                        android:id="@+id/student_3d"
                        android:layout_marginLeft="12dp"
                        android:text="6ယောက်"/>
                </RelativeLayout>

                <RelativeLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"

                    android:layout_marginLeft="16dp"
                    android:layout_marginTop="16dp"
                    android:layout_toRightOf="@+id/d3_layout"
                    android:id="@+id/d2_layout"
                    >
                    <RelativeLayout
                        android:layout_width="60dp"
                        android:layout_height="60dp"
                        android:id="@+id/d2"
                        android:layout_marginLeft="8dp"

                        android:background="@drawable/develop_pass_image"
                        >
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:elevation="6dp"
                            android:textStyle="bold"
                            android:textSize="32dp"
                            android:text="2D"
                            android:textColor="@color/white"
                            android:layout_centerInParent="true"/>
                    </RelativeLayout>
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/d2"
                        android:id="@+id/student_2d"
                        android:layout_marginTop="4dp"
                        android:layout_marginLeft="12dp"
                        android:text="6ယောက်"/>
                </RelativeLayout>



                <RelativeLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"

                    android:layout_marginLeft="16dp"
                    android:layout_toRightOf="@+id/d2_layout"
                    android:layout_marginTop="16dp"
                    android:id="@+id/d1_layout"

                    >
                    <RelativeLayout
                        android:layout_width="60dp"
                        android:layout_height="60dp"
                        android:id="@+id/d1"

                        android:layout_marginLeft="8dp"
                        android:background="@drawable/develop_pass_image"
                        >
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:elevation="6dp"
                            android:textStyle="bold"
                            android:textSize="32dp"
                            android:text="1D"
                            android:textColor="@color/white"
                            android:layout_centerInParent="true"/>
                    </RelativeLayout>

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/d1"
                        android:layout_marginTop="4dp"
                        android:id="@+id/student_1d"
                        android:layout_marginLeft="12dp"
                        android:text="6ယောက်"/>
                </RelativeLayout>
            </RelativeLayout>
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:layout_below="@+id/lowD"
                android:layout_marginTop="16dp"
                android:layout_centerHorizontal="true"
                android:id="@+id/phay">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/exam_student_title"
                    android:text="ဖြေဆိုသူဦးရေ -"
                    android:textSize="16dp"
                    />
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/exam_student"
                    android:text="56ယောက်"
                    android:textSize="16dp"
                    />
            </LinearLayout>
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:layout_below="@+id/phay"
                android:layout_marginTop="16dp"
                android:layout_centerHorizontal="true"
                android:id="@+id/phayy">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/pass_student_title"
                    android:text="အောင်မြင်သူဦးရေ - "
                    android:layout_marginRight="8dp"
                    android:textSize="16dp"

                    />
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/pass_student"
                    android:text="44ယောက်"
                    android:textSize="16dp"

                    />
            </LinearLayout>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="အောင်ချက်"
                android:textSize="27dp"
                android:layout_below="@+id/phayy"
                android:id="@+id/text_percent"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="10dp"
                />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/text_percent"
                android:layout_centerHorizontal="true"
                android:textSize="48dp"
                android:id="@+id/textPercent"
                android:textColor="@color/colorPrimaryDark"
                android:text="78.57%"
                android:layout_marginBottom="8dp"
                android:layout_marginTop="0dp"
                android:elevation="4dp"
                android:textStyle="bold"/>
        </RelativeLayout>

    </android.support.v7.widget.CardView>
</LinearLayout>